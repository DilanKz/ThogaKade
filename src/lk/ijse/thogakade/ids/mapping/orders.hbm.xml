<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="lk.ijse.thogakade.entity.Orders" table="customer-customID">
        <id name="id" column="orderID">
            <generator class="lk.ijse.thogakade.ids.OrdersIDGenerator"/>
        </id>
        <property name="date" column="orderDate"/>
        <property name="cusID" column="customerID"/>
    </class>
</hibernate-mapping>